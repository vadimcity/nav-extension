{

  "manifest_version": 2,
  "name": "Quester",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/quest-logo.svg"
  },

  "content_scripts": [
    {
      "matches": ["*://*.quester.io/*"],
      "js": ["quester-app.js"]
    }
  ]

}

